<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e28eb4"
    tools:context=".Front_page">


    <Button
        android:id="@+id/btn_doctor"
        android:layout_width="189dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="142dp"
        android:text="ডাক্তারের পরামর্শ"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_baby"
        android:layout_width="174dp"
        android:layout_height="77dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/btn_doctor"
        android:text="কেমন আছে আমার সন্তান "
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_food"
        android:layout_width="183dp"
        android:layout_height="68dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="197dp"
        android:layout_marginStart="0dp"
        android:text="কি খাবো"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_delivery_time"
        android:layout_width="180dp"
        android:layout_height="68dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/btn_food"
        android:text="কবে দেখব তোর মুখ"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_reminder"
        android:layout_width="182dp"
        android:layout_height="66dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button42"
        android:layout_marginBottom="104dp"
        android:text="মনে করিয়ে দেয়া"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_my_world"
        android:layout_width="171dp"
        android:layout_height="67dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/btn_reminder"
        android:text="তৈরী করি আমার জগত"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_immergency"
        android:layout_width="183dp"
        android:layout_height="69dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button40"
        android:layout_marginBottom="19dp"
        android:text="ইমারজেন্সি ব্যবস্থা"
        android:textSize="15dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btn_help"
        android:layout_width="174dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/btn_immergency"
        android:text="বিপদ বাটন"
        android:textSize="15dp"
        android:textStyle="normal" />
</RelativeLayout>